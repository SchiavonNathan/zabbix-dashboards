# Configurações de Conexão com a API do Zabbix
ZABBIX_SERVER=
ZABBIX_USER=
ZABBIX_PASSWORD=

# Chave secreta do Flask para mensagens flash (obrigatório para produção)
FLASK_SECRET_KEY=

# Configuração de ambiente (development ou production)
FLASK_ENV=production

# Porta para a aplicação (opcional, padrão 5001)
PORT=5001